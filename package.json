{
  "name": "punchout-ui-frontend",
  "version": "1.0.0",
  "description": "Next.js frontend for cXML Punchout Service",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "build:dev": "env-cmd -f .env.dev next build",
    "build:stage": "env-cmd -f .env.stage next build",
    "build:preprod": "env-cmd -f .env.preprod next build",
    "build:s4-dev": "env-cmd -f .env.s4-dev next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start": "next start -p 3000",
    "start:dev": "env-cmd -f .env.dev next start -p 3000",
    "start:stage": "env-cmd -f .env.stage next start -p 3000",
    "start:preprod": "env-cmd -f .env.preprod next start -p 3000",
    "start:s4-dev": "env-cmd -f .env.s4-dev next start -p 3000",
    "start:prod": "env-cmd -f .env.production next start -p 3000",
    "lint": "next lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "axios": "^1.6.0",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.1",
    "next": "14.0.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "env-cmd": "^10.1.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2"
  }
}
